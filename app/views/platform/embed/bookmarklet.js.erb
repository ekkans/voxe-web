url = '<%= plugins_compare_index_url(id: @election.id) %>';

e = document.createElement('script');
e.setAttribute('type', 'text/javascript');
e.setAttribute('charset', 'UTF-8');
e.setAttribute('src', 'http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js');
e.onload = function() {
  switch(window.location.hostname) {
    case 'www.lexpress.fr':
      $(".header-dossier h1").html("Comparer les candidats");
      $(".entete h2").html("avec Voxe.org");
      html = "<iframe src='" + url + "' width='645px' height='480px' style='border: 0; margin: 0'></iframe>";
      $('.contenu-article').html(html);
    break;
    case 'www.lemonde.fr':
      $(".col_gauche h1").html("Comparer les candidats");
      $("p.source").html("avec Voxe.org");
      html = "<iframe src='" + url + "' width='540' height='480px' style='border: 0; margin: 0'></iframe>";
      $('.contenu_article').html(html);
    break;
  }
};
document.body.appendChild(e);